{
  "version": 2,
  "builds": [
    {
      "src": "parcial3Be/**",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "buildCommand": "pip install -r requirements.txt && python -m compileall ."
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "parcial3Be/wsgi.py"
    }
  ]
}
